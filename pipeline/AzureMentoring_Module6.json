					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": "SELECT * FROM dbo.Tickets WHERE [transactionDt] > '@{activity('Lookup Get WaterMarkValue').output.firstRow.OldWmarkValue}'",
