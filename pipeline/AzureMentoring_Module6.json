{
	"name": "AzureMentoring_Module6",
	"properties": {
		"activities": [
			{
				"name": "Copy from MS SQL Tikets into Azure SQL movies",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Lookup Get WaterMarkValue",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
